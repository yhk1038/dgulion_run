<div class="col-md-2"></div>
<div class="panel panel-default col-md-8">
  <!-- Default panel contents -->
  <div class="panel-heading" style="font-size:25px;background: rgba(255, 255, 255, 0.7);">면접현황판</b></div>
  <div class="panel-body">
    <p>면접 시간 확인</p>
  </div>

  <!-- Table -->
  <table class="table">
    <thead>
        <tr style="font-weight:bold;">
            <td>시간대</td>
            <td>면접자1</td>
            <td>면접자2</td>
            <td>면접자3</td>
            <td>면접자4</td>
        </tr>
    </thead>
    <tbody>
        <% LTime.all.each do |a| %>
            <tr>
                <td><%= a.name %></td>
                <td><%= LUser.find(a.time1).name if a.time1 != 0 %></td>
                <td><%= LUser.find(a.time2).name if a.time2 != 0 %></td>
                <td><%= LUser.find(a.time3).name if a.time3 != 0 %></td>
                <td><%= LUser.find(a.time4).name if a.time4 != 0 %></td>
            </tr>
        <% end %>
    </tbody>
  </table>
</div>
<div class="col-md-2"></div>